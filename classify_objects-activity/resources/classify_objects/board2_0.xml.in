<shape name="violin" pixmapfile="classify_objects/mass/violin.png" position="0" sound="sounds/$LOCALE/classify_objects/violin.ogg" x="450" y="300">
<tooltip>violin</tooltip></shape>

<shape name="tree" pixmapfile="classify_objects/mass/tree.png" position="0" sound="sounds/$LOCALE/classify_objects/tree.ogg" x="650" y="300">
<tooltip>tree</tooltip></shape>


<shape name="background" pixmapfile="classify_objects/mass/background.png" position="0" sound="sounds/$LOCALE/classify_objects/background.ogg" type="SHAPE_BACKGROUND" x="400.0" y="260.0">
